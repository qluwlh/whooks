{
  "name": "whooks",
  "version": "0.0.2",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx, --format=pretty",
    "lint:prettier": "prettier --check \"*/**/*\" --end-of-line auto",
    "lint:fix": "yarn lint:js --fix && yarn prettier",
    "prettier": "yarn lint:prettier --write",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "standard-version",
    "publish": "npm run build && npm publish",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@umijs/test": "^3.0.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.0.16",
    "eslint": "^7.32.0",
    "eslint-config-soraka": "^0.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.4.0",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-soraka": "^0.1.1",
    "typescript": "^4.4.3",
    "yorkie": "^2.0.0"
  },
  "homepage": "https://github.com/wanglihua007/whooks#readme",
  "repository": "https://github.com/wanglihua007/whooks.git",
  "author": "wanglihua <wanglihua@innobuddy.com>",
  "bugs": {
    "url": "https://github.com/wanglihua007/whooks/issues"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
